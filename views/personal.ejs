<div class="twenty">
  <div class="twenty-page">
    <div class="twenty-overlap">
      <div class="twenty-rectangle"></div>
      <div class="rectangle-2"></div>
      <div class="text-wrapper">교통</div>
      <div class="rectangle-3"></div>
      <div class="text-wrapper-2">주유</div>
      <div class="rectangle-4"></div>
      <div class="text-wrapper-3">쇼핑</div>
      <div class="rectangle-5"></div>
      <div class="text-wrapper-4">카페</div>
      <div class="rectangle-6"></div>
      <div class="text-wrapper-5">통신</div>
      <div class="twenty-group">
        <% cards.forEach(card=> { %>
          <div class="twenty-overlap-group">
            <!-- 카드 이미지 -->
            <img class="twenty-image" src="<%= card.image_url || 'img/card-placeholder.png' %>" alt="카드 이미지" />

            <!-- 카드 이름 -->
            <div class="twenty-text-wrapper">
              <%= card.card_name %>
            </div>

            <!-- 카드 세부 정보 -->
            <div class="twenty-div">
              <div class="twenty-overlap-2">
                <p class="twenty-p">
                  <span class="twenty-span">전월실적<br /></span>
                  <span class="twenty-text-wrapper-2">
                    <%= card.monthly_spending_requirement || '조건 없음' %>
                  </span>
                </p>
                <p class="twenty-div-2">
                  <span class="twenty-span">연회비<br /></span>
                  <span class="twenty-text-wrapper-2">
                    <%= card.annual_fee %>
                  </span>
                </p>
                <div class="twenty-rectangle-2"></div>
                <div class="twenty-text-wrapper-4">상세보기</div>
              </div>

              <!-- 혜택 표시 -->
              <div class="twenty-text-wrapper-5">주요 혜택</div>
              <div class="twenty-element-CGV">
                <% card.benefit.split(',').forEach(benefit=> { %>
                  <div class="twenty-benefit-item">
                    <%= benefit.trim() %>
                  </div>
                  <% }); %>
              </div>
            </div>
          </div>
          <% }); %>
      </div>
      <div class="twenty-rectangle-3"></div>
      <div class="twenty-rectangle-4"></div>
      <div class="twenty-rectangle-5"></div>
      <div class="twenty-text-wrapper-6">
        <a href="/personal">신용카드</a>
      </div>
      <div class="twenty-text-wrapper-7">
        <a href="/personal-check">체크카드</a>
      </div>

      <div class="twenty-frame"></div>
    </div>
  </div>
</div>